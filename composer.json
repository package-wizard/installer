{
    "name": "andrey-helldar/package-wizard",
    "description": "Package Wizard is a composer plugin for creating a new packages using CLI tool.",
    "type": "composer-plugin",
    "license": "MIT",
    "keywords": [
        "composer",
        "init",
        "initializer",
        "laravel",
        "package",
        "packages",
        "plugin",
        "symfony",
        "wizard"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/package-wizard/issues",
        "source": "https://github.com/andrey-helldar/package-wizard"
    },
    "require": {
        "php": "^7.2.5|^8.0",
        "ext-json": "*",
        "composer-plugin-api": "^2.0",
        "andrey-helldar/support": "^2.0",
        "andrey-helldar/verbose": "^1.0",
        "symfony/console": "^5.0",
        "symfony/finder": "^5.0"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\PackageWizard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "class": "Helldar\\PackageWizard\\Application"
    }
}
